<?xml version="1.0" encoding="ISO-8859-1"?>
<project xmlns="http://maven.apache.org/DECORATION/1.4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.4.0 http://maven.apache.org/xsd/decoration-1.4.0.xsd"
         name="Trombone">
    <skin>
        <groupId>lt.velykis.maven.skins</groupId>
        <artifactId>reflow-maven-skin</artifactId>
        <version>1.1.1</version>
    </skin>

    <custom>
        <reflowSkin>
            <!--Do not display the creator of skin-->
            <skinAttribution>false</skinAttribution>
            <brand>
                <name>Trombone</name>
                <href>index.html</href>
            </brand>
            <navbarInverse>false</navbarInverse>
            <titleTemplate>%2$s | %1$s</titleTemplate>
            <breadcrumbs>false</breadcrumbs>
            <theme>bootswatch-cosmo</theme>
        </reflowSkin>
    </custom>

    <!-- Workaround for bug in skin that generates spurious closing header tag. -->
    <bannerRight>
        <name></name>
    </bannerRight>
    <bannerLeft>
        <name></name>
    </bannerLeft>

    <body>
        <head>
            <link href="http://fonts.googleapis.com/css?family=Source+Sans+Pro:400,700|Bitter:400,700" rel="stylesheet"
                  type="text/css"/>
            <link href="/css/override.css" rel="stylesheet" type="text/css"/>
        </head>

        <links>
            <item name="Home" href="index.html"/>
            <item name="Javadoc" href="https://builds.cs.st-andrews.ac.uk/job/trombone/javadoc/"/>
            <item name="Source" href="source-repository.html"/>
            <item name="Modules" href="modules.html"/>
            <item name="CI" href="integration.html"/>
            <item name="License" href="license.html"/>
            <item name="Team" href="team-list.html"/>
        </links>
        <menu name="Software">
            <!--<item name="Using Trombone" href="dependency-info.html"/>-->
        </menu>

    </body>

    <!--Show the last publish date at the bottom of the page-->
    <publishDate position="bottom" format="dd MMM yyyy"/>

    <!--Do not display the version-->
    <version position="none"/>
</project>
